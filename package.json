    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "mongoose": "^5.10.3",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.4"
  },
  "devDependencies": {
    "concurrently": "^5.3.0"
  }
}
